{% extends "::base.html.twig" %}

{% block title %}Admin - Ajouter un post{% endblock %}

{% block body %}
    <h1>Admin - Ajouter un post</h1>

    <form class="col-md-6" action="{{ path('admin_add_post') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <input type="submit" class="btn btn-success" value="Ajouter le post">
    </form>
{% endblock %}
